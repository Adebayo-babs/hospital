<%- include ('partials/header') %>

<div class="doctor_list">
    <h1>Welcome <%= username %> </h1>

   
    <% record.forEach(function(records){ %>
    <a href="/doctor/addPat">Add Patient</a>
    <a href="/doctor/viewPat">View Patient</a>
    <a href="/">Sign Out</a>
    <div class="logo_2">
     <img src="data:image/<%=records.upload.contentType%>;base64,<%=records.upload.data.toString('base64')%>" 
     width="200" height="200">
    </div>
    
    <p>Full Name: <b><%= records.fName + " " + records.lName  %></b></p>
    <p>Short description about yourself: <b><%= records.description %></b></p>
    <p>Contact Email: <b><%= records.cEmail %></b></p>
    <p>Contact Number: <b><%= records.cNumber %></b> </p>
    <p>Years of Experience: <b><%= records.yearsOfExp %></b></p>
    <p>Registered Date: <b><%= records.date.toLocaleDateString() %></b></p>
    <% }) %>
</div>



<%- include ('partials/footer') %>